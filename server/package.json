{
  "name": "rehouser-server",
  "version": "1.0.0",
  "main": "index.js",
  "author": "The Heaths",
  "license": "MIT",
  "scripts": {
    "start": "nodemon -e js,graphql -x node src/index.js",
    "dev": "nodemon -e js,graphql -x node --inspect src/index.js",
    "test": "jest",
    "deploy": "prisma deploy --env-file variables.env",
    "deploy-prod": "prisma deploy --env-file variables-prod.env",
    "reset": "prisma reset --env-file variables.env",
    "reset-prod": "prisma reset --env-file variables-prod.env",
    "prisma-dev-token": "prisma token --env-file variables.env",
    "generate-schema": "prisma generate --env-file variables.env",
    "format": "prettier \"**/*.jss\" --write"
  },
  "dependencies": {
    "algoliasearch": "^3.32.1",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.13.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "crawler": "^1.2.0",
    "docxtemplater": "^3.9.9",
    "dotenv": "^6.2.0",
    "fs": "^0.0.1-security",
    "graphql": "^14.1.1",
    "graphql-cli": "^3.0.9",
    "graphql-scalars": "^1.0.1",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "8.5.1",
    "jszip": "2",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "node-schedule": "^1.3.2",
    "nodemailer": "^5.1.1",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "prisma": "^1.34.10",
    "prisma-binding": "2.3.16",
    "stripe": "^6.23.0"
  },
  "repository": {
    "url": "dunatron.nz"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "devDependencies": {
    "prettier": "^1.18.2"
  }
}
