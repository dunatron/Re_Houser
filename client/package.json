    "start": "NODE_ENV=production node server.js",
    "test": "NODE_ENV=test jest --watch",
    "heroku-postbuild": "next build",
    "now-build": "next build",
    "format": "prettier \"**/*.{js,ts,tsx,css,graphql}\" --write",
    "apollo-graph-publish:dev": "npx apollo service:push --endpoint=http://localhost:7777",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve",
    "stripe:listen": "stripe listen --forward-to http://localhost:4444/stripe/webhook",
    "stripe:trigger:created": "stripe trigger payment_intent.created",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "lint": "eslint ./components/** ./pages/** -c .eslintrc.js --ext js,jsx",
    "lint-fix": "eslint ./components/** ./pages/** -c .eslintrc.js --fix --ext js,jsx",
